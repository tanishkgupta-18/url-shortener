{
  "manifest_version": 3,
  "name": "Go URL Shortener",
  "version": "1.0",
  "description": "Shorten URLs using your Go backend",
  "permissions": ["activeTab", "scripting", "clipboardWrite"],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icons/icon16.png",
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
    }
  }
}
